{
  "name": "frontend-technical-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-server": "json-server --watch src/server/db.json --routes src/server/routes.json --middlewares src/server/middleware/conversations.js --port 3005",
    "test": "jest",
    "lint": "npm run lint:ts && npm run lint:js",
    "lint:ts": "tsc",
    "lint:js": "eslint . --max-warnings=0 --ext .js,.ts,.tsx --cache --cache-location=node_modules/.cache/",
    "prettify": "eslint --fix \"./**/*.{js,ts,tsx}\"",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.17.2"
  },
  "dependencies": {
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "json-server": "^0.16.3",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.3.5",
    "husky": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn flow"
    }
  }
}
